{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@Pages/*": ["./src/pages/*"],
      "@Images/*": ["./src/assets/images/*"],
      "@Components/*": ["./src/omponents/*"]
    }
  }
}
